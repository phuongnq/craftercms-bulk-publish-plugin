{
  "private": true,
  "name": "craftercms-bulk-publish-plugin",
  "version": "1.0.0",
  "description": "A Crafter CMS plugin to perform bulk publish that can be used on Studio UI",
  "author": "phil.nguyen@craftercms.com",
  "license": "MIT",
  "repository": {
    "url": "phuongnq/craftercms-bulk-publish-plugin",
    "directory": "src"
  },
  "main": "dist/index.js",
  "module": "build/index.js",
  "typings": "build/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup --config rollup.config.cjs",
    "watch": "rollup --config rollup.config.cjs -w",
    "ts": "tsc -p tsconfig.json",
    "i18n": "run-s i18n:extract i18n:compile i18n:manage",
    "i18n:extract": "formatjs extract --extract-source-location true --out-file ./src/i18n/messages/extract.json src/**/*.{tsx,ts}",
    "i18n:compile": "formatjs compile --out-file ./src/i18n/translations/en.json ./src/i18n/messages/extract.json",
    "i18n:manage": "node manageTranslations.js"
  },
  "devDependencies": {
    "@formatjs/cli": "^2.15.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^16.7.13",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "npm-run-all": "^4.1.5",
    "react-intl-translations-manager": "^5.0.3",
    "rollup": "^4.12.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-replace-imports-with-vars": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@craftercms/studio-ui": "^4.1.2",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.15.10",
    "prettier": "^2.8.8",
    "query-string": "^6.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^5.25.1",
    "react-redux": "^7.2.9",
    "redux": "^4.2.1",
    "rxjs": "^7.8.1"
  }
}
